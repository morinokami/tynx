{
  "name": "tynx",
  "version": "0.0.1",
  "description": "tynx is a text-based web reader written in TypeScript.",
  "author": "Shinya Fujino <shf0811@gmail.com>",
  "license": "MIT",
  "keywords": [
    "browser",
    "terminal",
    "console",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/morinokami/tynx.git"
  },
  "main": "dist/index.js",
  "bin": {
    "tynx": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "tynx": "ts-node ./src/index.ts",
    "build": "yarn clean && tsc -p . && cp -r ./src/static ./dist",
    "clean": "rm -rf ./dist",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "validate": "npm-run-all --parallel build test lint",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.17",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/turndown": "^5.0.0",
    "@types/valid-url": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "meow": "^9.0.0",
    "puppeteer": "^8.0.0",
    "strip-ansi": "^6.0.0",
    "tmp-promise": "^3.0.2",
    "turndown": "^7.0.0"
  }
}
